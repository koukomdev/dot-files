[user]
    name = Koji Komiya
    email = koukomdev@gmail.com
[core]
    editor = vim -c \"set fenc=utf-8\"
    pager = less
    excludesfile = /Users/koji.komiya/.gitignore_global
[color]
    ui = true
[alias]
    co = checkout
    st = status
    ci = commit
    df = diff
    br = branch
    ad = add
    lo = log
    ps = push
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path =
[mergetool "sourcetree"]
    cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true
